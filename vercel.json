{
  "version": 2,
  "builds": [
    {
      "src": "api/send-reminder.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-reminder",
      "dest": "api/send-reminder.js"
    }
  ],
  "rewrites": [
    {
      "source": "/openapi.yaml",
      "destination": "/openapi.yaml"
    }
  ],
  "outputDirectory": "public"
}
